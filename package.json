  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@storyofams/eslint-config-ams/web"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|ts|tsx)": [
      "yarn lint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storyofams/react-ui.git"
  }
}
